{
  "extends": [
    "development"
  ],
  "hints": {
    "highest-available-document-mode": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-box-reflect"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/layout": "off"
  }
}